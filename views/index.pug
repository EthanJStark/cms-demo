extends layout

block content
  .jumbotron
    h1 ForkPitch Music
    p.lead All the music news you need, with none of the pretension
  .row
  each post in posts
    .col-lg-4
      h2(data-id=post.id)= post.title
      input.post__edit-title(data-id=post.id, type='text', name='edit-title', value=post.title).h2
  